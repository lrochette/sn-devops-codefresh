        <name>Change | Create</name>
        <operation_script><![CDATA[(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

	var result;
	var version = 'v1';
	
	var payload = request.body.data;
	
	if (! payload.cf_build_id) {
		response.setBody(CodefreshErrorResponse.MISSING_BUILD_ID);
		response.setStatus(404);
		return;
	} 
	
	result=(new CodefreshChangeHandler()).processCallback({version: version}, request.queryParams, payload);
	
	response.setBody(result || CodefreshErrorResponse.UNABLE_TO_PROCESS_ERROR);
	response.setStatus((result && result.responseCode) || 400);
	
	if(result && result.responseCode)
		delete result.responseCode;

})(request, response);]]></operation_script>
